---
import BaseLayout from '../../layouts/BaseLayout.astro';
import { getLangFromUrl } from '../../i18n/utils';

const lang = getLangFromUrl(Astro.url); // Will be 'en'

// SEO Configuration for Integrations Page
const title =
  'Platform Integrations | Praxis Navigator API & Enterprise Integrations';
const description =
  'Comprehensive integration capabilities for Praxis Navigator including REST API, SIEM integrations, business intelligence tools, SSO configuration, and enterprise security platform connectivity.';
const keywords =
  'Praxis Navigator integrations, REST API integration, SIEM integration, business intelligence integration, SSO single sign-on, enterprise security platform API, Microsoft Graph API integration';

// Structured Data for Software API
const apiSchema = {
  '@context': 'https://schema.org',
  '@type': 'WebAPI',
  name: 'Praxis Navigator Integration API',
  description:
    'REST API for integrating behavioral security monitoring with enterprise systems',
  url: 'https://praxisnavigator.io/product/integrations',
  provider: {
    '@type': 'Organization',
    name: 'Praxis Security Labs',
    url: 'https://praxisnavigator.io',
  },
  documentation: 'https://praxisnavigator.io/product/integrations',
};

// Breadcrumb Schema
const breadcrumbSchema = {
  '@context': 'https://schema.org',
  '@type': 'BreadcrumbList',
  itemListElement: [
    {
      '@type': 'ListItem',
      position: 1,
      name: 'Home',
      item: 'https://praxisnavigator.io',
    },
    {
      '@type': 'ListItem',
      position: 2,
      name: 'Product',
      item: 'https://praxisnavigator.io/product',
    },
    {
      '@type': 'ListItem',
      position: 3,
      name: 'Integrations',
      item: 'https://praxisnavigator.io/product/integrations',
    },
  ],
};

// Combine structured data
const combinedSchema = {
  '@context': 'https://schema.org',
  '@graph': [apiSchema, breadcrumbSchema],
} as Record<string, unknown>;
---

<BaseLayout
  title={title}
  description={description}
  keywords={keywords}
  pageType="product"
  structuredData={combinedSchema}
  ogImage="/images/product/integrations-og.png"
  ogType="website"
  lang={lang}
>
  <!-- Breadcrumb Navigation -->
  <nav
    aria-label="Breadcrumb"
    class="bg-praxis-gray-50 border-b border-praxis-gray-200"
  >
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-3">
      <ol class="flex items-center space-x-2 text-sm">
        <li>
          <a
            href="/"
            class="text-praxis-brown hover:text-praxis-dark-blue transition-colors"
          >
            Home
          </a>
        </li>
        <li class="text-praxis-gray">
          <span aria-hidden="true">/</span>
        </li>
        <li>
          <a
            href="/product"
            class="text-praxis-brown hover:text-praxis-dark-blue transition-colors"
          >
            Product
          </a>
        </li>
        <li class="text-praxis-gray">
          <span aria-hidden="true">/</span>
        </li>
        <li aria-current="page" class="text-praxis-dark-blue font-medium">
          Integrations
        </li>
      </ol>
    </div>
  </nav>

  <!-- Hero Section -->
  <section class="py-20 bg-gradient-to-b from-praxis-blue to-praxis-sky">
    <div class="container mx-auto px-6">
      <div class="max-w-4xl mx-auto text-center">
        <h1 class="h1 text-praxis-white mb-8">Platform Integrations & API</h1>

        <h2 class="h3 text-praxis-gold mb-8">
          Seamless Integration with Your Existing Enterprise Security Stack
