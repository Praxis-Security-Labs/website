---
import BaseLayout from '../../layouts/BaseLayout.astro';
import { ComparisonTable } from '../../components/sections/ComparisonTable';
import { CompetitorNarrative } from '../../components/sections/CompetitorNarrative';
import { PricingComparison } from '../../components/sections/PricingComparison';
import { PersonaCTA } from '../../components/ui/PersonaCTA';
import { junglemapComparisonData } from '../../content/comparisons/junglemap';

const title =
  'Praxis Navigator vs Junglemap - Security Analytics Platform Comparison';
const description =
  'Compare Praxis Navigator and Junglemap for cybersecurity analytics and posture management. See features, pricing, and key differences between specialized vs comprehensive approaches.';
---

<BaseLayout
  title={title}
  description={description}
  canonicalUrl="/compare/junglemap"
  keywords="praxis navigator vs junglemap, cybersecurity posture management, security culture analytics, nimblr comparison, security platform comparison"
>
  <main>
    <div class="container mx-auto px-4 py-8 lg:py-16">
      <!-- Hero Section -->
      <div class="text-center mb-16">
        <div class="flex items-center justify-center space-x-6 mb-8">
          <img
            src="/images/praxis-logo.svg"
            alt="Praxis Navigator"
            class="h-12 w-auto"
          />
          <span class="text-4xl font-heading font-bold text-praxis-dark-blue"
            >vs</span
          >
          <img
            src="/images/competitors/junglemap-logo.svg"
            alt="Junglemap"
            class="h-12 w-auto"
          />
        </div>
        <h1
          class="text-4xl md:text-5xl lg:text-6xl font-heading font-bold text-praxis-dark-blue mb-6"
        >
          Praxis Navigator vs Junglemap
        </h1>
        <p
          class="text-xl text-praxis-blue-700 leading-relaxed max-w-4xl mx-auto"
        >
          Compare specialized security culture analytics with comprehensive
          cybersecurity posture management. Understand whether you need focused
          behavioral insights or broad security visibility.
        </p>
      </div>

      <!-- Quick Overview Cards -->
      <div class="grid md:grid-cols-2 gap-8 mb-16">
        <div
          class="bg-gradient-to-br from-praxis-accent to-praxis-accent-dark rounded-2xl p-8 text-white"
        >
          <div class="flex items-center space-x-3 mb-4">
            <img
              src="/images/praxis-logo-white.svg"
              alt="Praxis Navigator"
              class="h-8 w-auto"
            />
            <h2 class="text-2xl font-heading font-bold">Praxis Navigator</h2>
          </div>
          <p class="text-lg mb-6 opacity-95">
            Specialized security culture analytics for Microsoft 365
            environments with deep behavioral insights.
          </p>
          <ul class="space-y-2">
            <li class="flex items-center space-x-2">
              <svg
                class="w-5 h-5 text-white"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M5 13l4 4L19 7"></path>
              </svg>
              <span>Microsoft 365 specialization</span>
            </li>
            <li class="flex items-center space-x-2">
              <svg
                class="w-5 h-5 text-white"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M5 13l4 4L19 7"></path>
              </svg>
              <span>Security culture measurement</span>
            </li>
            <li class="flex items-center space-x-2">
              <svg
                class="w-5 h-5 text-white"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M5 13l4 4L19 7"></path>
              </svg>
              <span>Behavioral risk analytics</span>
            </li>
            <li class="flex items-center space-x-2">
              <svg
                class="w-5 h-5 text-white"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M5 13l4 4L19 7"></path>
              </svg>
              <span>Zero data storage</span>
            </li>
          </ul>
        </div>

        <div class="bg-white border-2 border-praxis-blue-200 rounded-2xl p-8">
          <div class="flex items-center space-x-3 mb-4">
            <img
              src="/images/competitors/junglemap-logo.svg"
              alt="Junglemap"
              class="h-8 w-auto"
            />
            <h2 class="text-2xl font-heading font-bold text-praxis-dark-blue">
              Junglemap
            </h2>
          </div>
          <p class="text-lg mb-6 text-praxis-blue-700">
            Comprehensive cybersecurity posture management platform with broad
            integration capabilities across security tools.
          </p>
          <ul class="space-y-2">
            <li class="flex items-center space-x-2">
              <svg
                class="w-5 h-5 text-praxis-accent"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M5 13l4 4L19 7"></path>
              </svg>
              <span class="text-praxis-dark-blue"
                >Full security posture visibility</span
              >
            </li>
            <li class="flex items-center space-x-2">
              <svg
                class="w-5 h-5 text-praxis-accent"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M5 13l4 4L19 7"></path>
              </svg>
              <span class="text-praxis-dark-blue"
                >Multi-source data integration</span
              >
            </li>
            <li class="flex items-center space-x-2">
              <svg
                class="w-5 h-5 text-praxis-accent"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M5 13l4 4L19 7"></path>
              </svg>
              <span class="text-praxis-dark-blue"
                >Comprehensive risk assessment</span
              >
            </li>
            <li class="flex items-center space-x-2">
              <svg
                class="w-5 h-5 text-praxis-accent"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M5 13l4 4L19 7"></path>
              </svg>
              <span class="text-praxis-dark-blue"
                >Enterprise security operations</span
              >
            </li>
          </ul>
        </div>
      </div>

      <!-- Feature Comparison Table -->
      <section class="mb-16">
        <ComparisonTable
          competitorName={junglemapComparisonData.competitorName}
          competitorLogo={junglemapComparisonData.competitorLogo}
          features={junglemapComparisonData.features}
          language="en"
          client:load
        />
      </section>

      <!-- Narrative Comparison -->
      <section class="mb-16">
        <CompetitorNarrative
          competitorName={junglemapComparisonData.competitorName}
          competitorLogo={junglemapComparisonData.competitorLogo}
          introduction={junglemapComparisonData.narrative.introduction}
          sections={junglemapComparisonData.narrative.sections}
          conclusion={junglemapComparisonData.narrative.conclusion}
          language="en"
          client:load
        />
      </section>

      <!-- Pricing Comparison -->
      <section class="mb-16">
        <PricingComparison
          competitorName={junglemapComparisonData.competitorName}
          competitorLogo={junglemapComparisonData.competitorLogo}
          tiers={junglemapComparisonData.pricing.tiers}
          additionalComparisons={junglemapComparisonData.pricing
            .additionalComparisons}
          language="en"
          client:load
        />
      </section>

      <!-- Persona-Specific CTA -->
      <section class="mb-16">
        <div
          class="bg-gradient-to-br from-praxis-blue-50 to-white rounded-2xl p-8 lg:p-12"
        >
          <div class="text-center mb-8">
            <h2
              class="text-3xl md:text-4xl font-heading font-bold text-praxis-dark-blue mb-4"
            >
              See Specialized Security Culture Analytics
            </h2>
            <p class="text-xl text-praxis-blue-700 max-w-3xl mx-auto">
              Discover how focused behavioral analytics complement broad
              security posture management. See the depth of insights possible
              with specialized Microsoft 365 culture measurement.
            </p>
          </div>

          <PersonaCTA
            segment="security-leaders"
            size="lg"
            variant="comparison"
            customHeadline="Experience Behavioral Security Analytics"
            language="en"
            client:load
          />
        </div>
      </section>

      <!-- Complementary Use Case -->
      <section class="mb-16">
        <div class="bg-praxis-dark-blue rounded-2xl p-8 lg:p-12 text-white">
          <div class="text-center mb-8">
            <h2 class="text-3xl md:text-4xl font-heading font-bold mb-4">
              Better Together: Complementary Security Insights
            </h2>
            <p class="text-xl opacity-95 max-w-3xl mx-auto">
              Many organizations use both platforms for comprehensive security
              visibility - Junglemap for broad posture management and Praxis
              Navigator for deep behavioral insights.
            </p>
          </div>

          <div class="grid md:grid-cols-2 gap-8">
            <div class="bg-white bg-opacity-10 rounded-xl p-6">
              <h3 class="text-xl font-heading font-bold mb-4">
                Junglemap Strengths
              </h3>
              <ul class="space-y-2 text-praxis-blue-100">
                <li>• Comprehensive security posture visibility</li>
                <li>• Multi-source data correlation</li>
                <li>• Enterprise security operations support</li>
                <li>• Broad technology stack coverage</li>
              </ul>
            </div>

            <div class="bg-white bg-opacity-10 rounded-xl p-6">
              <h3 class="text-xl font-heading font-bold mb-4">
                Praxis Navigator Strengths
              </h3>
              <ul class="space-y-2 text-praxis-blue-100">
                <li>• Deep behavioral security insights</li>
                <li>• Security culture measurement & tracking</li>
                <li>• Microsoft 365 native specialization</li>
                <li>• Real-time behavioral risk assessment</li>
              </ul>
            </div>
          </div>
        </div>
      </section>

      <!-- Related Comparisons -->
      <section>
        <div class="text-center mb-8">
          <h2
            class="text-2xl md:text-3xl font-heading font-bold text-praxis-dark-blue mb-4"
          >
            Other Platform Comparisons
          </h2>
        </div>

        <div class="grid md:grid-cols-2 gap-6">
          <a
            href="/compare/knowbe4"
            class="group bg-white rounded-xl border border-praxis-blue-200 p-6 hover:border-praxis-accent transition-colors"
          >
            <div class="flex items-center space-x-4 mb-4">
              <img
                src="/images/praxis-logo.svg"
                alt="Praxis Navigator"
                class="h-6 w-auto"
              />
              <span class="text-lg font-medium text-praxis-dark-blue">vs</span>
              <img
                src="/images/competitors/knowbe4-logo.svg"
                alt="KnowBe4"
                class="h-6 w-auto"
              />
            </div>
            <h3
              class="text-lg font-heading font-bold text-praxis-dark-blue mb-2 group-hover:text-praxis-accent transition-colors"
            >
              Praxis Navigator vs KnowBe4
            </h3>
            <p class="text-praxis-blue-700">
              Compare security culture analytics with established security
              awareness training platforms.
            </p>
          </a>

          <a
            href="/compare"
            class="group bg-white rounded-xl border border-praxis-blue-200 p-6 hover:border-praxis-accent transition-colors"
          >
            <div class="text-center">
              <svg
                class="w-12 h-12 text-praxis-blue-400 mx-auto mb-4 group-hover:text-praxis-accent transition-colors"
                fill="none"
                viewBox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v4a2 2 0 01-2 2h-2a2 2 0 01-2-2z"
                ></path>
              </svg>
              <h3
                class="text-lg font-heading font-bold text-praxis-dark-blue mb-2 group-hover:text-praxis-accent transition-colors"
              >
                View All Comparisons
              </h3>
              <p class="text-praxis-blue-700">
                See how Praxis Navigator compares with other security platforms
                and solutions.
              </p>
            </div>
          </a>
        </div>
      </section>
    </div>
  </main>
</BaseLayout>
