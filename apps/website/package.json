{
  "name": "praxis-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "build:fast": "astro build",
    "build:with-budget": "node scripts/build-with-budget.js",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "analyze:bundle": "vite-bundle-analyzer dist/_astro/*.js --open false",
    "preview": "astro preview",
    "lint": "eslint . --ext .ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .ts,.tsx,.astro --fix",
    "type-check": "astro check",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:watch": "vitest",
    "test:all": "npm run lint && npm run type-check && npm run test:unit && npm run build:fast",
    "test:lighthouse": "npm run build && lhci autorun",
    "test:lighthouse:ci": "lhci autorun --config=./lighthouserc.js",
    "lighthouse:open": "lhci open",
    "clean": "rm -rf dist .astro",
    "astro": "astro",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.0",
    "@astrojs/react": "^3.0.0",
    "@astrojs/tailwind": "^5.0.0",
    "@azure/msal-browser": "^4.21.0",
    "@hookform/resolvers": "^5.2.1",
    "@sentry/astro": "^10.5.0",
    "@sentry/browser": "^10.5.0",
    "astro": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.62.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^3.5.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.37.0",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.34.0",
    "astro-eslint-parser": "^0.16.0",
    "eslint": "^8.0.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jsdom": "^22.0.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.12.0",
    "rollup-plugin-bundle-analyzer": "^1.6.6",
    "vite-bundle-analyzer": "^1.2.1",
    "vitest": "^0.34.0"
  }
}
