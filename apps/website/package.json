{
  "name": "praxis-website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "build:fast": "astro build",
    "build:with-budget": "node scripts/build-with-budget.js",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "analyze:bundle": "vite-bundle-analyzer dist/_astro/*.js --open false",
    "preview": "astro preview",
    "lint": "eslint . --ext .ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .ts,.tsx,.astro --fix",
    "type-check": "astro check",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:watch": "vitest",
    "test:all": "npm run lint && npm run type-check && npm run test:unit && npm run build:fast",
    "test:lighthouse": "npm run build && lhci autorun",
    "test:lighthouse:ci": "lhci autorun --config=./lighthouserc.js",
    "lighthouse:open": "lhci open",
    "clean": "rm -rf dist .astro",
    "astro": "astro",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "@azure/msal-browser": "^4.21.0",
    "@sentry/astro": "^10.5.0",
    "@sentry/browser": "^10.5.0",
    "astro": "^5.13.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "web-vitals": "^5.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^3.5.1",
    "@eslint/js": "^9.33.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "rollup-plugin-bundle-analyzer": "^1.6.6",
    "vite-bundle-analyzer": "^1.2.1",
    "vitest": "^3.2.4"
  }
}
