{
  "name": "praxis-website-monorepo",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Praxis Security Labs promotional website monorepo",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/website",
    "build": "npm run build --workspace=apps/website",
    "preview": "npm run preview --workspace=apps/website",
    "lint": "npm run lint --workspace=apps/website",
    "lint:fix": "npm run lint:fix --workspace=apps/website",
    "type-check": "npm run type-check --workspace=apps/website",
    "test": "npm run test --workspace=apps/website",
    "test:unit": "npm run test:unit --workspace=apps/website",
    "test:e2e": "npm run test:e2e --workspace=apps/website",
    "test:all": "npm run test:all --workspace=apps/website",
    "prepare": "husky"
  },
  "author": "Praxis Security Labs",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
